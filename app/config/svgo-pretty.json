{
  "multipass": true,
  "full": true,
  "plugins": [
    {"removeDoctype": true},
    {"removeXMLProcInst": true},
    {"removeComments": true},
    {"removeMetadata": true},
    {"removeXMLNS": true},
    {"removeEditorsNSData": true},
